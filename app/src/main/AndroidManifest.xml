<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.umairmumtaz.connectyou">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activities.ZeroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activities.FirstPage" />
        <activity
            android:name=".view.activities.Start"
            android:parentActivityName=".view.activities.FirstPage" />
        <activity
            android:name=".view.activities.Instructions"
            android:parentActivityName=".view.activities.FirstPage" />
        <activity
            android:name=".view.activities.About"
            android:parentActivityName=".view.activities.FirstPage" />
        <activity
            android:name=".view.activities.ContactUs"
            android:parentActivityName=".view.activities.FirstPage" />
        <activity
            android:name=".view.activities.ChatActivity"
            android:parentActivityName=".view.activities.Start" />
    </application>

</manifest>